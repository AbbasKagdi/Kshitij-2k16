<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kshitij2k16"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.WRITE_PROFILE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.kshitij2k16.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.kshitij2k16.FrontPage"
            android:label="@string/title_activity_front_page" >
        </activity>
        <activity
            android:name="com.kshitij2k16.EventList"
            android:label="@string/title_activity_event_list" >
        </activity>
        <activity
            android:name="com.kshitij2k16.CoOrdinators"
            android:label="@string/title_activity_co_ordinators" >
        </activity>
        <activity
            android:name="com.kshitij2k16.EventGallery"
            android:label="@string/title_activity_event_gallery" >
        </activity>
        <activity
            android:name="com.kshitij2k16.AboutKshitij"
            android:label="@string/title_activity_about_kshitij" >
        </activity>
        <activity
            android:name="com.kshitij2k16.EventDetail"
            android:label="@string/title_activity_event_detail" >
        </activity>
    </application>

</manifest>